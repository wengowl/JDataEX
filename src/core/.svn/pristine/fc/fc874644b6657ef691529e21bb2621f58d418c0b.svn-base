package org.jdataex.channel.exception;

/**
 * 所有类型的错误都将继承error,error为RuntimeException类型
 * @author chaos
 *
 */
public class BaseError extends RuntimeException {

	public BaseError(Throwable cause) {
		super(cause.getMessage(), cause);
	}
	
	public BaseError(String error) {
		super(error);
	}

	/**
	 * 
	 */
	private static final long serialVersionUID = -6835943590784603570L;

}
