package org.jdataex.flow.component;

import java.util.List;

import org.jdataex.flow.handler.IComponentHandler;
import org.jdataex.flow.runner.IComponentRunner;

public interface IComponentLinkInfo {

	public abstract void removeNextComponent(int index);

	public abstract void addNextComponent(IComponent component);

	public abstract List<IComponent> getNextComponents();

	public abstract void setComponent(IComponent component);

	public abstract IComponent getComponent();

	public abstract void setHandler(IComponentHandler handler);

	public abstract IComponentHandler getHandler();

	public abstract void setRunner(IComponentRunner runner);

	public abstract IComponentRunner getRunner();

}
